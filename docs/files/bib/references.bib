	pages = {2339--2349},
	file = {PubMed Central Full Text PDF:C\:\\Users\\IMMCCAR\\Zotero\\storage\\H89LRTX8\\Postler et al. - 2018 - Prevalence and treatment of hip and knee osteoarth.pdf:application/pdf},
