	author = {Ketcham, Jonathan D. and Kuminoff, Nicolai V. and Powers, Christopher A.},
	month = dec,
	year = {2016},
	pages = {3932--3961},
	file = {Full Text PDF:C\:\\Users\\IMMCCAR\\Zotero\\storage\\3ZWESNDD\\Ketcham et al. - 2016 - Choice Inconsistencies among the Elderly Evidence.pdf:application/pdf;Snapshot:C\:\\Users\\IMMCCAR\\Zotero\\storage\\LM57YNMY\\articles.html:text/html;Snapshot:C\:\\Users\\IMMCCAR\\Zotero\\storage\\7ZDV3D6D\\articles.html:text/html},
