	journal = {Journal of Rural Health},
	author = {Kaufman, Brystana G. and Thomas, Sharita R and Randolph, Randy and Perry, Julie R. and Thompson, Kristie Weisner and {Kristie W. Thompson} and Holmes, George M. and Pink, George H.},
