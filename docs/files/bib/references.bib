	year = {2002},
	keywords = {Humans, United States, Health Services Accessibility, Health Services Research, Socioeconomic Factors, Attitude of Health Personnel, Decision Making, Ethnicity, Physician-Patient Relations, Prejudice, Stereotyping},
	pages = {I140--151},
}

@article{whiteis_dg_hospital_1992,
	title = {Hospital and community characteristics in closures of urban hospitals, 1980-87},
