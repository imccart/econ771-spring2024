@article{shepard2022,
	title = {Hospital {Network} {Competition} and {Adverse} {Selection}: {Evidence} from the {Massachusetts} {Health} {Insurance} {Exchange}},
	volume = {112},
	issn = {0002-8282},
	shorttitle = {Hospital {Network} {Competition} and {Adverse} {Selection}},
	url = {https://pubs.aeaweb.org/doi/10.1257/aer.20201453},
