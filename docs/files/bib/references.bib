@article{sloan_antecedents_2003,
	title = {Antecedents of hospital ownership conversions, mergers, and closures.},
	volume = {40},
	doi = {10.5034/inquiryjrnl_40.1.39},
	abstract = {This study assesses the determinants of conversions in hospital ownership from 1986 through 1996. To place such changes in context, we also analyze causes of hospital mergers and closures, which are often alternatives to hospital ownership conversion. A consistent result from our analysis is that an important antecedent of ownership conversions is a low profit margin. Conversions from private nonprofit or government ownership to for-profit status are preceded by chronically low margins and high debt-to-asset ratios. By contrast, conversions from for-profit ownership occur quickly following declines in margins. Many mergers seem motivated by a desire to increase market power—a consideration not evident for conversions. The U.S. hospital industry experienced a major downsizing during the last two decades of the 20th century, gauged in terms of both the number of hospitals and the number of hospital beds. Between 1980 and 2000, the number of hospitals declined by 17\% and the number of hospital beds by 28\%; most of the decline occurred from 1986 to 1996, the period under study in this paper (National Center for Health Statistics 1998, 2002). The decreases in hospitals and hospital beds reflect a virtual halt in hospital entry, coupled with a substantial number of exits and mergers. At the same time, the composition of hospitals by ownership form has changed, with public hospitals losing and private for-profit hospitals gaining market share (American Hospital Association 2001). Changes in the composition of hospital ownership accompany sales of hospitals and reflect differential rates of exit by ownership form. Among the major forces underlying these trends appear to be increased competition in the product markets in which hospitals operate, and a related change—a switch from paying hospitals for inpatient care on a retrospective cost basis to paying via various forms of prospective payment. Also important is technological change enabling the substitution of outpatient care for inpatient care. To a certain extent, such technological change was a response to the changes in payment practices for inpatient care. Interestingly, although there is widespread consensus about the factors responsible for the trends in organization of the hospital industry, there is a paucity of empirical studies documenting the underlying relationships.},
	number = {1},
	journal = {Inquiry},
	author = {Sloan, Frank A. and Ostermann, Jan and Conover, Christopher J.},
	month = feb,
	year = {2003},
	doi = {10.5034/inquiryjrnl_40.1.39},
	pmid = {12836907},
	note = {MAG ID: 2099897846},
	pages = {39--56},
}

