	author = {Chernozhukov, Victor and Hansen, Christian},
	year = {2005},
	keywords = {bounded completeness, Endogeneity, identification, instrumental regression, monotone likelihood ratio, nonlinear model, partial identification, quantile regression, simultaneous equations},
	pages = {245--261},
	file = {Full Text PDF:C\:\\Users\\IMMCCAR\\Zotero\\storage\\4TJEK7IU\\Chernozhukov and Hansen - 2005 - An IV Model of Quantile Treatment Effects.pdf:application/pdf;Snapshot:C\:\\Users\\IMMCCAR\\Zotero\\storage\\ASE8RQA6\\j.1468-0262.2005.00570.html:text/html},
}

