@article{tebaldi_nonparametric_2023,
	title = {Nonparametric {Estimates} of {Demand} in the {California} {Health} {Insurance} {Exchange}},
	volume = {91},
	issn = {1468-0262},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.3982/ECTA17215},
	doi = {10.3982/ECTA17215},
	abstract = {We develop a new nonparametric approach for discrete choice and use it to analyze the demand for health insurance in the California Affordable Care Act marketplace. The model allows for endogenous prices and instrumental variables, while avoiding parametric functional form assumptions about the unobserved components of utility. We use the approach to estimate bounds on the effects of changing premiums or subsidies on coverage choices, consumer surplus, and government spending on subsidies. We find that a {\textbackslash}10 decrease in monthly premium subsidies would cause a decline of between 1.8\% and 6.7\% in the proportion of subsidized adults with coverage. The reduction in total annual consumer surplus would be between {\textbackslash}62 and {\textbackslash}74 million, while the savings in yearly subsidy outlays would be between {\textbackslash}207 and \$602 million. We estimate the demand impacts of linking subsidies to age, finding that shifting subsidies from older to younger buyers would increase average consumer surplus, with potentially large impacts on enrollment. We also estimate the consumer surplus impact of removing the highly-subsidized plans in the Silver metal tier, where we find that a nonparametric model is consistent with a wide range of possibilities. We find that comparable mixed logit models tend to yield price sensitivity estimates toward the lower end of the nonparametric bounds, while producing consumer surplus impacts that can be both higher and lower than the nonparametric bounds depending on the specification of random coefficients.},
	language = {en},
	number = {1},
	urldate = {2023-04-24},
	journal = {Econometrica},
	author = {Tebaldi, Pietro and Torgovitsky, Alexander and Yang, Hanbin},
	year = {2023},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA17215},
	keywords = {health insurance, demand estimation, partial identification, ACA, discrete choice models, linear programming, Nonparametric estimation},
	pages = {107--146},
	file = {Full Text:C\:\\Users\\IMMCCAR\\Zotero\\storage\\R4Y3DIZ7\\Tebaldi et al. - 2023 - Nonparametric Estimates of Demand in the Californi.pdf:application/pdf;Snapshot:C\:\\Users\\IMMCCAR\\Zotero\\storage\\4R9GCWXQ\\ECTA17215.html:text/html},
