	file = {Abowd Kramarz and Margolis 1999.pdf:C\:\\Users\\IMMCCAR\\Zotero\\storage\\5JWL77ZG\\Abowd Kramarz and Margolis 1999.pdf:application/pdf;Full Text PDF:C\:\\Users\\IMMCCAR\\Zotero\\storage\\A2A2MJDL\\Abowd et al. - 1999 - High Wage Workers and High Wage Firms.pdf:application/pdf;Snapshot:C\:\\Users\\IMMCCAR\\Zotero\\storage\\TRZDSHRR\\1468-0262.html:text/html;Snapshot:C\:\\Users\\IMMCCAR\\Zotero\\storage\\K52YGINY\\1468-0262.html:text/html},
